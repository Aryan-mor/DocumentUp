#!coffee

# Server's own configuration.
server:
  port: 8080

# Specify namespace prefix, hostname and port for Redis server.
redis:
  namespace:  "docup"
  hostname:   "localhost"
  port:       6379

github:
  client_id:     "YOUR GITHUB CLIENT ID"
  client_secret: "YOUR GITHUB CLIENT SECRET"

loggly:
  subdomain: "YOUR LOGGLY SUBDOMAIN"
  inputToken: "YOUR LOGGLY INPUT TOKEN"